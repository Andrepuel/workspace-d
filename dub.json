{
	"name": "workspace-d",
	"description": "A minimal D application.",
	"copyright": "Copyright Â© 2015, webfreak",
	"authors": [
		"webfreak"
	],
	"dependencies": {
		"dub": "~>0.9.25-beta.3",
		"dunit": "~>1.0.12",
		"painlessjson": "~>1.3.7"
	},
	"configurations": [
		{
			"name": "application",
			"sourcePaths": [
				"source"
			],
			"targetType": "executable"
		}
	],
	"buildTypes": {
		"fix-unittest": {
			"dflags": [
				"-unittest"
			],
			"buildOptions": [
				"warnings"
			],
			"buildRequirements": [
				"allowWarnings",
				"silenceWarnings"
			]
		}
	},
	"subPackages": [
		"./test",
		"./dml"
	]
}